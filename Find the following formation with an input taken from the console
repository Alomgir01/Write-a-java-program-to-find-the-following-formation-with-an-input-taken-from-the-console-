import java.util.Scanner;
public class lab4 {
public static void main(String[] args) {
Scanner input = new Scanner(System.input);
int i,j;
System.out.print("&quot;Enter the number: ");
int n = in.nextInt();
for(i=1;i&lt;=n;i++)
{

for(j=1;j&lt;=n;j++)
{
System.out.print(+i+" ");
}
System.out.print("\n";);
}
}
}
